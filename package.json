    "@ionic/core": "^6.2.0",
    "@ionic/vue": "^6.2.0",
    "@ionic/vue-router": "^6.2.0",
    "ionicons": "^6.0.2",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@aparajita/eslint-config-vue": "^1.0.5",
    "@aparajita/prettier-config": "^1.0.0",
    "@aparajita/tailwind-ionic": "^1.1.0",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@capacitor/cli": "^4.0.1",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue": "3.0.1",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.20.0",
