{
  "name": "@aparajita/capacitor-dark-mode-demo",
  "version": "1.0.0",
  "description": "An Ionic/Vue demo of the @aparajita/capacitor-dark-mode plugin",
  "private": true,
  "author": "Aparajita Fishman",
  "homepage": "https://github.com/aparajita/capacitor-dark-mode-demo",
  "repository": {
    "type": "git",
