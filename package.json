    "lint.fix": "eslint --cache --fix . && pnpm typecheck",
    "typecheck": "vue-tsc --noEmit",
    "prettier": "prettier --check .",
    "prettier.fix": "prettier --check --write .",
    "check": "pnpm lint && pnpm prettier && echo '\u001b[32m✔︎\u001b[0mAll good!'",
    "check.fix": "pnpm lint.fix && pnpm prettier.fix && echo '\u001b[32m✔︎\u001b[0mAll good!'",
    "ios.dev": "ionic cap run ios --open --livereload --external",
    "ios.prod": "ionic cap run ios --open",
    "android.dev": "ionic cap run android --open --livereload --external",
    "android.prod": "ionic cap run android --open",
    "ionic:build": "vite build",
    "ionic:serve": "vite"
  },
  "dependencies": {
    "@aparajita/capacitor-dark-mode": "^2.0.0",
    "@capacitor/android": "^4.0.1",
